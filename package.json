{
  "name": "ntlstl.admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "build": "npm run clear && webpack --mode production --env production --config webpack.prod.ts",
    "dev": "webpack serve --mode development --open --hot --env development --config webpack.dev.ts",
    "lint": "npx eslint ./src",
    "slint": "npx stylelint '**/*.css' --fix",
    "clear": "rm -rf build/",
    "predeploy": "npm run build",
    "test": "jest",
    "prepare:run": "npm run cy:run --record --key --spec & npm run build",
    "dockerize": "docker-compose build",
    "start-container": "docker-compose up"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
